{
  "name": "video-transcripts-to-docs",
  "version": "1.0.0",
  "description": "AI-powered knowledge extraction from video transcripts",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "build": "node scripts/build.js",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "typecheck": "jsdoc -t templates/haruki src/**/*.js"
  },
  "keywords": ["ai", "knowledge-extraction", "video", "transcripts", "documentation"],
  "author": "Claude Flow Community",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.57.0",
    "jsdoc": "^4.0.2"
  },
  "dependencies": {
    "uuid": "^9.0.1",
    "natural": "^6.12.0",
    "levenshtein": "^1.0.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/**/*.test.{ts,js}",
      "!src/**/index.{ts,js}"
    ],
    "coverageReporters": ["text", "lcov", "html"],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "**/tests/**/*.test.{ts,js}"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": ["ts", "js", "json"],
    "testTimeout": 30000
  }
}